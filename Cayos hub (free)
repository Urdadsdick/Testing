if game.PlaceId == 11177482306 then
    -- Display Intro Message
    print("Goated Hub for Street Wars 2")

    -- Load Orion UI
    local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source.lua'))()

    -- Create Orion Window
    local Window = OrionLib:MakeWindow({
        Name = "Goated Hub for Street Wars 2",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "GoatedHubConfig"
    })

    -- Initialize Global Variables
    _G.autoMop = false
    _G.autoPickup = false
    _G.pickupRange = 5000000
    _G.onePunch = false
    _G.godMode = false
    _G.flying = false
    _G.speed = 50
    _G.noclip = false

    -- Function to Kill All Players except Local Player
    local function killAllPlayers()
        for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= game.Players.LocalPlayer and player.Character then
                local humanoid = player.Character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.Health = 0
                end
            end
        end
    end

    -- Function to Kill a Specific Player
    local function killPlayer(playerToKill)
        if playerToKill.Character then
            local humanoid = playerToKill.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.Health = 0
            end
        end
    end

    -- Player Tab
    local playerTab = Window:MakeTab({
        Name = "Player",
        Icon = "rbxassetid://4483345998",  -- You can replace this with your own icon
        PremiumOnly = false
    })
    
    -- Walk Speed Slider
    playerTab:AddSlider({
        Name = "Walk Speed",
        Min = 16,
        Max = 100,
        Default = _G.speed,
        Callback = function(value)
            _G.speed = value
            local player = game.Players.LocalPlayer
            local humanoid = player.Character and player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = _G.speed
            end
        end
    })

    -- Fly/Unfly Buttons
    playerTab:AddButton({
        Name = "Fly",
        Callback = function()
            startFly()
        end
    })

    playerTab:AddButton({
        Name = "Unfly",
        Callback = function()
            stopFly()
        end
    })

    -- Teleports Tab
    local teleportsTab = Window:MakeTab({
        Name = "Teleports",
        Icon = "rbxassetid://4483345998",  -- You can replace this with your own icon
        PremiumOnly = false
    })

    teleportsTab:AddButton({
        Name = "Teleport to Wheel",
        Callback = function()
            local wheelPosition = Vector3.new(0, 10, 0) -- Change this to actual wheel location
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(wheelPosition)
        end
    })

    -- Troll Tab
    local trollTab = Window:MakeTab({
        Name = "Troll",
        Icon = "rbxassetid://4483345998",  -- You can replace this with your own icon
        PremiumOnly = false
    })

    -- Kill All Players Button
    trollTab:AddButton({
        Name = "Kill All Players",
        Callback = function()
            killAllPlayers()
        end
    })

    -- Kill Player Dropdown (List of Players in Server)
    local playersList = {}
    for _, player in pairs(game.Players:GetPlayers()) do
        table.insert(playersList, player.Name)
    end

    trollTab:AddDropdown({
        Name = "Kill Player",
        Options = playersList,
        Callback = function(selectedPlayer)
            for _, player in pairs(game.Players:GetPlayers()) do
                if player.Name == selectedPlayer then
                    killPlayer(player)
                    break
                end
            end
        end
    })

    -- Script Tab with Infinite Yield Toggle
    local scriptTab = Window:MakeTab({
        Name = "Script",
        Icon = "rbxassetid://4483345998",  -- You can replace this with your own icon
        PremiumOnly = false
    })

    -- Toggle for Infinite Yield
    scriptTab:AddToggle({
        Name = "Infinite Yield",
        Default = false,
        Callback = function(enabled)
            if enabled then
                -- Execute Infinite Yield when toggled on
                loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
            end
        end
    })
end
