local keyRequired = "key"  -- The key that must be entered to unlock the script
local keyUrl = "https://discord.gg/ADQUB6W7"  -- Link for "Get Key"

-- Function to initialize the script's features
local function loadScriptFeatures()
    -- Load the Orion library
    local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()
    local Window = OrionLib:MakeWindow({
        Name = "Goated Hub - Key System",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "GoatedHub"
    })
    
    -- Player Settings
    local players = game:GetService("Players")
    local plr = players.LocalPlayer
    local workspace = game:GetService("Workspace")
    local replicatedStorage = game:GetService("ReplicatedStorage")

    local settings = {
        walkSpeed = 16,
        godMode = false,
        onePunch = false,
        autoPickup = false,
        autoFarm = false,
        espEnabled = false,
        flyEnabled = false
    }

    -- Utility: Dynamic Random Delay
    local random = Random.new()
    local function rnd(min, max) return random:NextNumber(min, max) end

    --- CORE FUNCTIONS ---

    -- Custom Damage Integration (Bypassing)
    local function dealDamage(targetPlayer)
        local character = targetPlayer and targetPlayer.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.Health = 0 -- Instantly set health to zero
            end
        end
    end

    -- God Mode
    local function enableGodMode()
        settings.godMode = true
        while settings.godMode do
            local character = plr.Character
            if character then
                local humanoid = character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.Health = humanoid.MaxHealth -- Continuously restore health
                end
            end
            wait(0.1)
        end
    end

    local function disableGodMode()
        settings.godMode = false
    end

    -- One Punch Kill
    local function enableOnePunch()
        settings.onePunch = true
        while settings.onePunch do
            for _, player in pairs(players:GetPlayers()) do
                if player ~= plr and player.Character then
                    dealDamage(player) -- Use bypassed damage logic
                end
            end
            wait(rnd(0.1, 0.3))
        end
    end

    local function disableOnePunch()
        settings.onePunch = false
    end

    -- Auto Pickup
    local function enableAutoPickup()
        settings.autoPickup = true
        while settings.autoPickup do
            local root = plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
            if root then
                for _, item in pairs(workspace:GetDescendants()) do
                    if item:IsA("Tool") or item.Name == "Cash" then
                        pcall(function()
                            item.CFrame = root.CFrame -- Move item to the player's position
                        end)
                    end
                end
            end
            wait(rnd(0.5, 1))
        end
    end

    local function disableAutoPickup()
        settings.autoPickup = false
    end

    -- ESP (Highlighting)
    local function enableESP()
        settings.espEnabled = true
        while settings.espEnabled do
            for _, player in pairs(players:GetPlayers()) do
                if player ~= plr and player.Character then
                    local highlight = player.Character:FindFirstChildOfClass("Highlight") or Instance.new("Highlight", player.Character)
                    highlight.FillColor = Color3.new(rnd(0, 1), rnd(0, 1), rnd(0, 1))
                    highlight.OutlineColor = Color3.new(1, 1, 1)
                end
            end
            wait(rnd(0.3, 0.5))
        end
    end

    local function disableESP()
        settings.espEnabled = false
    end

    -- Flying
    local flying = false
    local function startFly()
        if flying then return end
        flying = true
        local root = plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
        if not root then return end

        local bodyGyro = Instance.new("BodyGyro", root)
        local bodyVelocity = Instance.new("BodyVelocity", root)

        bodyGyro.P = 9e4
        bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        bodyGyro.CFrame = root.CFrame

        bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        bodyVelocity.Velocity = Vector3.zero

        game:GetService("RunService").RenderStepped:Connect(function()
            if flying then
                bodyGyro.CFrame = root.CFrame
                bodyVelocity.Velocity = root.CFrame.LookVector * 50
            else
                bodyGyro:Destroy()
                bodyVelocity:Destroy()
            end
        end)
    end

    local function stopFly()
        flying = false
    end

    --- TELEPORT TAB ---
    local teleportTab = Window:MakeTab({ Name = "Teleport", Icon = "rbxassetid://4483345998", PremiumOnly = false })

    teleportTab:AddButton({
        Name = "Apt2",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(2569.16, 34.81, -115.59))
        end
    })

    teleportTab:AddButton({
        Name = "Car Dealer",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(796.04, 4.39, -15.37))
        end
    })

    teleportTab:AddButton({
        Name = "Shoe Shop",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(55.09, 4.00, -345.23))
        end
    })

    teleportTab:AddButton({
        Name = "Father & Sons",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(57.11, 4.00, -321.91))
        end
    })

    teleportTab:AddButton({
        Name = "Tattoo Shop",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(56.30, 4.00, -288.71))
        end
    })

    teleportTab:AddButton({
        Name = "Pharmacy",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(56.85, 4.00, -247.30))
        end
    })

    teleportTab:AddButton({
        Name = "Ice House",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(56.09, 4.00, -225.75))
        end
    })

    teleportTab:AddButton({
        Name = "Kingz Cutz",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(138.54, 4.24, 155.02))
        end
    })

    teleportTab:AddButton({
        Name = "Kebab House",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(139.82, 4.24, 118.07))
        end
    })

    teleportTab:AddButton({
        Name = "Spin Wheel",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(142.29, 4.24, 85.71))
        end
    })

    teleportTab:AddButton({
        Name = "Dominoes",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(137.71, 4.24, 46.26))
        end
    })

    teleportTab:AddButton({
        Name = "Kiko",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(150.60, 4.24, -56.72))
        end
    })

    teleportTab:AddButton({
        Name = "Beauty Hair Salon",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(151.31, 4.24, -89.81))
        end
    })

    teleportTab:AddButton({
        Name = "Apt1",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(4.34, 4.04, 50.51))
        end
    })

    teleportTab:AddButton({
        Name = "London Drip",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(-163.27, 4.19, 33.05))
        end
    })

    teleportTab:AddButton({
        Name = "JD",
        Callback = function()
            plr.Character:MoveTo(Vector3.new(-141.78, 4.22, 39.92))
        end
    })

    --- PLAYERS TAB ---
    local playersTab = Window:MakeTab({ Name = "Players", Icon = "rbxassetid://4483345998", PremiumOnly = false })

    playersTab:AddToggle({
        Name = "One Punch Kill",
        Default = false,
        Callback = function(value)
            if value then
                enableOnePunch()
            else
                disableOnePunch()
            end
        end
    })

    --- Features Tab ---
    local featuresTab = Window:MakeTab({ Name = "Features", Icon = "rbxassetid://4483345998", PremiumOnly = false })

    featuresTab:AddToggle({
        Name = "God Mode",
        Default = false,
        Callback = function(value)
            if value then
                enableGodMode()
            else
                disableGodMode()
            end
        end
    })

    featuresTab:AddToggle({
        Name = "Auto Pickup",
        Default = false,
        Callback = function(value)
            if value then
                enableAutoPickup()
            else
                disableAutoPickup()
            end
        end
    })

    featuresTab:AddToggle({
        Name = "ESP",
        Default = false,
        Callback = function(value)
            if value then
                enableESP()
            else
                disableESP()
            end
        end
    })

    featuresTab:AddButton({
        Name = "Start Fly",
        Callback = function()
            startFly()
        end
    })

    featuresTab:AddButton({
        Name = "Stop Fly",
        Callback = function()
            stopFly()
        end
    })
    
end

-- Ensure the user is prompted for the key
local function checkKey()
    -- Get key input
    local keyInput = game:GetService("UserInputService"):InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode[keyRequired] then
            loadScriptFeatures()  -- Load the script once the key is entered
        end
    end)
end

checkKey()
