if game.PlaceId == 11177482306 then
    -- Display Intro Message
    print("Cayos Hub for Street Wars 2")

    -- Load OrionLib
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

    -- Create a Window for the GUI
    local Window = OrionLib:MakeWindow({
        Name = "Cayos Hub for Street Wars 2",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "OrionTest"
    })

    -- Initialize Global Values
    _G.speed = 50
    _G.flying = false
    _G.infiniteAmmo = false
    _G.autoPickup = false
    _G.spinning = false
    _G.godmode = false
    _G.onePunch = false
    _G.nameESP = false
    _G.autoMop = false

    -- Function: Fly
    local function startFly()
        if _G.flying then return end
        _G.flying = true
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local bodyGyro = Instance.new("BodyGyro", humanoidRootPart)
        local bodyVelocity = Instance.new("BodyVelocity", humanoidRootPart)
        bodyGyro.P = 9e4
        bodyGyro.MaxTorque = Vector3.new(9e4, 9e4, 9e4)
        bodyGyro.CFrame = humanoidRootPart.CFrame
        bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        bodyVelocity.Velocity = Vector3.zero

        local flySpeed = 50
        while _G.flying do
            bodyGyro.CFrame = workspace.CurrentCamera.CFrame
            bodyVelocity.Velocity = workspace.CurrentCamera.CFrame.LookVector * flySpeed
            task.wait()
        end

        bodyGyro:Destroy()
        bodyVelocity:Destroy()
    end

    -- Function: Stop Fly
    local function stopFly()
        _G.flying = false
    end

    -- Improved God Mode: Complete Invincibility
    local function enableGodMode()
        if _G.godmode then return end
        _G.godmode = true
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChild("Humanoid")

        -- Monitor and restore health
        humanoid:GetPropertyChangedSignal("Health"):Connect(function()
            if _G.godmode and humanoid.Health < humanoid.MaxHealth then
                humanoid.Health = humanoid.MaxHealth
            end
        end)

        -- Prevent all damage effects
        character.DescendantAdded:Connect(function(descendant)
            if _G.godmode and descendant:IsA("Humanoid") and descendant ~= humanoid then
                descendant.Health = humanoid.MaxHealth
            end
        end)
    end

    -- Function: Disable Godmode
    local function disableGodMode()
        _G.godmode = false
    end

    -- Function: Enable One Punch
    local function enableOnePunch()
        if _G.onePunch then return end
        _G.onePunch = true
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        humanoidRootPart.Touched:Connect(function(hit)
            local otherPlayer = game.Players:GetPlayerFromCharacter(hit.Parent)
            if otherPlayer and otherPlayer ~= player then
                local otherHumanoid = hit.Parent:FindFirstChild("Humanoid")
                if otherHumanoid then
                    otherHumanoid.Health = 0 -- Set the other player's health to 0
                end
            end
        end)
    end

    -- Function: Disable One Punch
    local function disableOnePunch()
        _G.onePunch = false
    end

    -- Function: Automatic Mop Autofarm
    local function startMopAutofarm()
        if _G.autoMop then return end
        _G.autoMop = true
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()

        while _G.autoMop do
            local backpack = player:FindFirstChild("Backpack")
            local mop = backpack and backpack:FindFirstChild("Mop")

            if mop then
                character.Humanoid:EquipTool(mop)

                for _, spot in pairs(workspace.LittlePakiShop.CleaningSpots:GetChildren()) do
                    if spot:IsA("Part") and spot:FindFirstChild("Cleanable") then
                        character.HumanoidRootPart.CFrame = spot.CFrame
                        task.wait(1) -- Simulate cleaning
                        firetouchinterest(character.HumanoidRootPart, spot, 0)
                        firetouchinterest(character.HumanoidRootPart, spot, 1)
                    end
                end
            else
                warn("Mop not found! Check inventory.")
                break
            end

            task.wait(5) -- Delay between loops
        end
    end

    local function stopMopAutofarm()
        _G.autoMop = false
    end

    -- Useful Tab
    local usefulTab = Window:MakeTab({
        Name = "Useful",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    -- Godmode Toggle
    usefulTab:AddToggle({
        Name = "Enable Godmode",
        Default = false,
        Callback = function(state)
            if state then
                enableGodMode()
            else
                disableGodMode()
            end
        end
    })

    -- Name ESP Toggle
    usefulTab:AddToggle({
        Name = "Enable Name ESP",
        Default = false,
        Callback = function(state)
            if state then
                enableNameESP()
            else
                disableNameESP()
            end
        end
    })

    -- Autofarm Tab
    local autofarmTab = Window:MakeTab({
        Name = "Autofarm",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    -- Mop Autofarm Toggle
    autofarmTab:AddToggle({
        Name = "Enable Mop Autofarm",
        Default = false,
        Callback = function(state)
            if state then
                startMopAutofarm()
            else
                stopMopAutofarm()
            end
        end
    })

    -- Creds Tab
    local credsTab = Window:MakeTab({
        Name = "Creds",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    credsTab:AddLabel("Developed by: Your Name")
    credsTab:AddLabel("Script by: Your Name")
    credsTab:AddLabel("Special Thanks to: Community and Supporters")

    -- Misc Tab
    local miscTab = Window:MakeTab({
        Name = "Misc",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    miscTab:AddButton({
        Name = "Reset Player",
        Callback = function()
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            character:BreakJoints() -- Resets the player
        end
    })

    -- Other Tab
    local otherTab = Window:MakeTab({
        Name = "Other",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    otherTab:AddButton({
        Name = "Print Player's Info",
        Callback = function()
            local player = game.Players.LocalPlayer
            print("Player Name: " .. player.Name)
            print("Player User ID: " .. player.UserId)
        end
    })

    -- Initialize GUI
    OrionLib:Init()
end
