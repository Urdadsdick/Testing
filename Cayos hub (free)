if game.PlaceId == 11177482306 then
    -- Debug Message
    print("Goated Hub for Street Wars 2 is loading...")

    -- Load Orion UI Library
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

    -- Create Orion Window
    local Window = OrionLib:MakeWindow({
        Name = "Goated Hub for Street Wars 2",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "GoatedHubConfig"
    })

    -- Initialize Global Variables
    _G.autoMop = false
    _G.speed = 50
    _G.flying = false
    _G.noclip = false

    -- Debug Tab Initialization
    print("Initializing Player Tab...")

    -- Player Tab
    local playerTab = Window:MakeTab({Name = "Player", Icon = "rbxassetid://4483345998"})

    playerTab:AddSlider({
        Name = "Walk Speed",
        Min = 16,
        Max = 100,
        Default = _G.speed,
        Callback = function(value)
            _G.speed = value
            local humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = _G.speed
            end
        end
    })

    playerTab:AddButton({
        Name = "Fly",
        Callback = function()
            print("Fly enabled (implement flying script)")
        end
    })

    playerTab:AddButton({
        Name = "Unfly",
        Callback = function()
            print("Fly disabled (implement flying script)")
        end
    })

    -- Debug Teleports Tab
    print("Initializing Teleports Tab...")

    -- Teleports Tab
    local teleportsTab = Window:MakeTab({Name = "Teleports", Icon = "rbxassetid://4483345998"})

    teleportsTab:AddButton({
        Name = "Teleport to Wheel",
        Callback = function()
            local wheelPosition = Vector3.new(0, 10, 0) -- Adjust coordinates to wheel location
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(wheelPosition)
        end
    })

    -- Debug Autofarm Tab
    print("Initializing Autofarm Tab...")

    -- Autofarm Tab
    local autoTab = Window:MakeTab({Name = "Autofarm", Icon = "rbxassetid://4483345998"})

    autoTab:AddToggle({
        Name = "Auto Mop Clean",
        Default = false,
        Callback = function(value)
            _G.autoMop = value
            if value then
                print("Auto Mop Enabled")
            else
                print("Auto Mop Disabled")
            end
        end
    })

    -- Debug Infinite Yield Tab
    print("Initializing Script Tab...")

    -- Script Tab
    local scriptTab = Window:MakeTab({Name = "Script", Icon = "rbxassetid://4483345998"})

    scriptTab:AddToggle({
        Name = "Infinite Yield",
        Default = false,
        Callback = function(enabled)
            if enabled then
                -- Execute Infinite Yield when toggled on
                loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
                print("Infinite Yield Executed")
            end
        end
    })

    -- Initialize Orion UI
    print("Goated Hub initialized!")
    OrionLib:Init()
end
