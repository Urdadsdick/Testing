if game.PlaceId == 11177482306 then
    -- Load Orion UI
    local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()

    -- Create Main Window
    local Window = OrionLib:MakeWindow({
        Name = "Goated Hub - Street Wars 2",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "GoatedHub"
    })

    -- Initialize Variables
    local _G = {
        autoFarm = false,
        autoPickup = false,
        godMode = false,
        espEnabled = false,
        walkSpeed = 16,
        infiniteAmmo = false,
        noclip = false,
        onePunch = false
    }

    -- Credits Tab
    local creditsTab = Window:MakeTab({
        Name = "Credits",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    creditsTab:AddLabel("Development by yooo0684 on DC")
    creditsTab:AddButton({
        Name = "Join Discord",
        Callback = function()
            setclipboard("https://discord.gg/DvgnsUp3")
        end
    })

    -- Player Tab
    local playerTab = Window:MakeTab({
        Name = "Player",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    -- Walk Speed Slider and Text Box
    playerTab:AddSlider({
        Name = "Walk Speed",
        Min = 16,
        Max = 100,
        Default = 16,
        Increment = 1,
        Callback = function(value)
            _G.walkSpeed = value
            if game.Players.LocalPlayer.Character then
                local humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.WalkSpeed = value
                end
            end
        end
    })

    playerTab:AddTextbox({
        Name = "Walk Speed (Mobile)",
        Default = "16",
        TextDisappear = true,
        Callback = function(value)
            _G.walkSpeed = tonumber(value)
            if game.Players.LocalPlayer.Character then
                local humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.WalkSpeed = _G.walkSpeed
                end
            end
        end
    })

    -- Fly and Unfly
    local flying = false
    local function startFly()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        flying = true

        spawn(function()
            while flying do
                humanoidRootPart.Velocity = humanoidRootPart.CFrame.LookVector * 100
                wait()
            end
        end)
    end

    local function stopFly()
        flying = false
    end

    playerTab:AddButton({
        Name = "Fly",
        Callback = function()
            startFly()
        end
    })

    playerTab:AddButton({
        Name = "Unfly",
        Callback = function()
            stopFly()
        end
    })

    -- God Mode
    playerTab:AddToggle({
        Name = "God Mode",
        Default = false,
        Callback = function(state)
            _G.godMode = state
            if state then
                local player = game.Players.LocalPlayer
                local character = player.Character or player.CharacterAdded:Wait()

                for _, obj in pairs(character:GetDescendants()) do
                    if obj:IsA("Humanoid") then
                        obj:SetAttribute("Invincible", true)
                        obj:GetPropertyChangedSignal("Health"):Connect(function()
                            if obj:GetAttribute("Invincible") and obj.Health < obj.MaxHealth then
                                obj.Health = obj.MaxHealth
                            end
                        end)
                    end
                end
            end
        end
    })

    -- Noclip Toggle
    playerTab:AddToggle({
        Name = "Noclip",
        Default = false,
        Callback = function(state)
            _G.noclip = state
            while _G.noclip do
                for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") and part.CanCollide then
                        part.CanCollide = false
                    end
                end
                wait()
            end
        end
    })

    -- Farming Tab
    local farmingTab = Window:MakeTab({
        Name = "Farming",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    farmingTab:AddToggle({
        Name = "Auto Farm Mop",
        Default = false,
        Callback = function(state)
            _G.autoFarm = state
            while _G.autoFarm do
                local mop = game.Players.LocalPlayer.Backpack:FindFirstChild("Mop") or game.Players.LocalPlayer.Character:FindFirstChild("Mop")
                if mop then
                    local floorSpots = workspace:FindFirstChild("FloorSpots") or {}
                    for _, spot in pairs(floorSpots:GetChildren()) do
                        if spot:FindFirstChild("Prompt") then
                            fireproximityprompt(spot.Prompt)
                        end
                    end
                end
                wait(0.5)
            end
        end
    })

    -- ESP Tab
    local espTab = Window:MakeTab({
        Name = "ESP",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    espTab:AddToggle({
        Name = "Enable ESP",
        Default = false,
        Callback = function(state)
            _G.espEnabled = state
            while _G.espEnabled do
                for _, player in pairs(game.Players:GetPlayers()) do
                    if player ~= game.Players.LocalPlayer and player.Character then
                        local highlight = player.Character:FindFirstChildOfClass("Highlight") or Instance.new("Highlight", player.Character)
                        highlight.FillColor = Color3.new(0, 1, 0)
                        highlight.OutlineColor = Color3.new(1, 1, 1)
                    end
                end
                wait(0.5)
            end
        end
    })

    -- Combat Tab
    local combatTab = Window:MakeTab({
        Name = "Combat",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    combatTab:AddToggle({
        Name = "Infinite Ammo",
        Default = false,
        Callback = function(state)
            _G.infiniteAmmo = state
            while _G.infiniteAmmo do
                for _, tool in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if tool:FindFirstChild("Ammo") then
                        tool.Ammo.Value = math.huge
                    end
                end
                wait(0.1)
            end
        end
    })

    -- Trolling Tab
    local trollingTab = Window:MakeTab({
        Name = "Trolling",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    trollingTab:AddToggle({
        Name = "One Punch (Bypass Damage Scripts)",
        Default = false,
        Callback = function(state)
            _G.onePunch = state
            if state then
                local player = game.Players.LocalPlayer
                local character = player.Character or player.CharacterAdded:Wait()
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")

                if humanoidRootPart then
                    humanoidRootPart.Touched:Connect(function(hit)
                        local hitPlayer = game.Players:GetPlayerFromCharacter(hit.Parent)
                        if hitPlayer and hitPlayer ~= player then
                            local humanoid = hit.Parent:FindFirstChild("Humanoid")
                            if humanoid then
                                humanoid.Health = 0
                            end

                            -- Destroy potential damage scripts on their character
                            for _, obj in pairs(hit.Parent:GetDescendants()) do
                                if obj:IsA("Script") or obj:IsA("LocalScript") then
                                    if string.find(obj.Name:lower(), "damage") or string.find(obj.Source:lower(), "damage") then
                                        obj:Destroy()
                                    end
                                end
                            end
                        end
                    end)
                end
            end
        end
    })

    -- Initialize UI
    OrionLib:Init()
end
